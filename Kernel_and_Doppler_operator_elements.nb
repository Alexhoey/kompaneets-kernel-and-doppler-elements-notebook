Cell["Recursion module (general m)", "Item",
 CellChangeTimes->{
  3.9749675734358664`*^9, {3.974967743812055*^9, 
   3.97496774423223*^9}},ExpressionUUID->"44b28d7c-ce30-4687-a00c-\
dc18d17ead62"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Cfunc", "[", 
    RowBox[{"l_", ",", "m_", ",", "s_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"l", "==", "0"}], "&&", 
      RowBox[{"m", "==", "s", "==", "0"}]}], ",", "1", ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"l", ">=", "m"}], "&&", 
        RowBox[{"l", ">=", "s"}]}], ",", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"l", "^", "2"}], "-", 
            RowBox[{"m", "^", "2"}]}], ")"}], "*", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"l", "^", "2"}], "-", 
             RowBox[{"s", "^", "2"}]}], ")"}], "/", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"4", "*", 
              RowBox[{"l", "^", "2"}]}], "-", "1"}], ")"}]}]}], "]"}], "/", 
        "l"}], ",", "0"}], "]"}], ",", "0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"KllT", "[", 
    RowBox[{"m_", ",", "lmax_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"l", ",", "lp", ",", 
       RowBox[{"rT", "=", 
        RowBox[{"ConstantArray", "[", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"lmax", "+", "1"}], ",", 
            RowBox[{"lmax", "+", "1"}]}], "}"}]}], "]"}]}], ",", "GetK", ",", 
       "SetK", ",", "Kmmmin", ",", "Kp1", ",", " ", "Ks"}], "}"}], ",", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "make", " ", "sure", " ", "the", " ", "variable", " ", "d", " ", "is", " ",
        "not", " ", "set", " ", "to", " ", "some", " ", "value"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Clear", "[", "d", "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "define", " ", "functions", " ", "to", " ", "access", " ", "right", " ",
         "array", " ", "elements"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"GetK", "[", 
        RowBox[{"l_", ",", "lp_"}], "]"}], ":=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"l", ">=", "m"}], "&&", 
          RowBox[{"lp", ">=", "m"}]}], ",", 
         RowBox[{"rT", "[", 
          RowBox[{"[", 
           RowBox[{
            RowBox[{"l", "+", "1"}], ",", 
            RowBox[{"lp", "+", "1"}]}], "]"}], "]"}], ",", "0"}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"SetK", "[", 
        RowBox[{"l_", ",", "lp_", ",", "val_"}], "]"}], ":=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"l", ">=", "m"}], "&&", 
          RowBox[{"lp", ">=", "m"}]}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"rT", "[", 
            RowBox[{"[", 
             RowBox[{
              RowBox[{"l", "+", "1"}], ",", 
              RowBox[{"lp", "+", "1"}]}], "]"}], "]"}], "=", "val"}], ";"}], ",", 
         RowBox[{
          RowBox[{"rT", "[", 
           RowBox[{"[", 
            RowBox[{
             RowBox[{"l", "+", "1"}], ",", 
             RowBox[{"lp", "+", "1"}]}], "]"}], "]"}], "=", "0"}]}], "]"}]}], 
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"initial", " ", "condition", " ", 
        RowBox[{"K", "[", 
         RowBox[{"m", ",", "m"}], "]"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Kmmmin", "=", 
       RowBox[{
        RowBox[{"FullSimplify", "[", 
         RowBox[{
          RowBox[{"FunctionExpand", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Kmmm", "[", 
              RowBox[{"m", ",", "d"}], "]"}], "/.", 
             RowBox[{"\[Gamma]", "->", 
              RowBox[{"Sqrt", "[", 
               RowBox[{"1", "+", 
                RowBox[{"p", "^", "2"}]}], "]"}]}]}], ",", 
            RowBox[{"p", ">", "0"}]}], "]"}], ",", 
          RowBox[{"p", ">", "0"}]}], "]"}], "/.", 
        RowBox[{
         SqrtBox[
          RowBox[{"1", "+", 
           SuperscriptBox["p", "2"]}]], "->", "\[Gamma]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"SetK", "[", 
       RowBox[{"m", ",", "m", ",", "Kmmmin"}], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"K", "[", 
           RowBox[{"l", ",", "lp"}], "]"}], " ", "will", " ", "be", " ", 
          "generated", " ", "starting", " ", "by", " ", "raising", " ", 
          RowBox[{
           RowBox[{"K", "[", 
            RowBox[{"m", ",", "m"}], "]"}], " ", "--"}]}], ">", " ", 
         RowBox[{
          RowBox[{"K", "[", 
           RowBox[{
            RowBox[{"m", "+", "1"}], ",", "m"}], "]"}], " ", "--"}], ">"}], 
        " ", "..."}], " ", "*)"}], " ", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"lp", "=", "m"}], ",", 
        RowBox[{"lp", "<=", "lmax"}], ",", 
        RowBox[{"lp", "++"}], ",", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"l", "=", 
            RowBox[{"Max", "[", 
             RowBox[{"lp", ",", 
              RowBox[{"m", "+", "1"}]}], "]"}]}], ",", 
           RowBox[{"l", "<=", "lmax"}], ",", 
           RowBox[{"l", "++"}], ",", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"l", ">", "0"}], ",", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"Kp1", "=", 
                RowBox[{
                 RowBox[{
                  RowBox[{"-", 
                   FractionBox["1", 
                    RowBox[{"Cfunc", "[", 
                    RowBox[{"l", ",", "m", ",", "0"}], "]"}]]}], "*", 
                  FractionBox[
                   RowBox[{
                    RowBox[{"\[Gamma]", "*", 
                    RowBox[{"GetK", "[", 
                    RowBox[{
                    RowBox[{"l", "-", "1"}], ",", "lp"}], "]"}]}], "-", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"GetK", "[", 
                    RowBox[{
                    RowBox[{"l", "-", "1"}], ",", "lp"}], "]"}], "/.", 
                    RowBox[{"d", "->", 
                    RowBox[{"d", "-", "1"}]}]}], ")"}]}], "p"]}], "-", 
                 RowBox[{
                  FractionBox[
                   RowBox[{"Cfunc", "[", 
                    RowBox[{
                    RowBox[{"l", "-", "1"}], ",", "m", ",", "0"}], "]"}], 
                   RowBox[{"Cfunc", "[", 
                    RowBox[{"l", ",", "m", ",", "0"}], "]"}]], "*", 
                  RowBox[{"GetK", "[", 
                   RowBox[{
                    RowBox[{"l", "-", "2"}], ",", "lp"}], "]"}]}]}]}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"SetK", "[", 
                RowBox[{"l", ",", "lp", ",", "Kp1"}], "]"}], ";"}]}], 
             "                                          ", 
             RowBox[{"(*", " ", 
              RowBox[{"save", " ", "value"}], " ", "*)"}], 
             "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
            "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"l", "!=", "lp"}], ",", 
              RowBox[{
               RowBox[{"SetK", "[", 
                RowBox[{"lp", ",", "l", ",", 
                 RowBox[{
                  RowBox[{"Kp1", "/.", 
                   RowBox[{"d", "->", 
                    RowBox[{"2", "-", "d"}]}]}], "/.", 
                  RowBox[{"p", "->", 
                   RowBox[{"-", "p"}]}]}]}], "]"}], ";"}]}], " ", 
             RowBox[{"(*", " ", 
              RowBox[{
              "directly", " ", "set", " ", "switched", " ", "kernel", " ", 
               "value"}], " ", "*)"}], "]"}], ";"}]}], "\[IndentingNewLine]", 
          "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}],
       ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "rT", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.974967593970533*^9, 3.974967595043844*^9}},
 CellLabel->
  "In[1558]:=",ExpressionUUID->"c8735b0b-276f-4eb8-95ca-32d86ec078a4"]
